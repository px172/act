<mujoco>
    <include file="scene.xml"/>
    <include file="vx300s_dependencies.xml"/>
    
    <worldbody>
        <include file="vx300s_left.xml" />
        <include file="vx300s_right.xml" />

        <body name="TUBE_STAND" pos="0.0 .8 0.01">
            <joint name="tube_stand_joint" type="free" frictionloss="0.01"/>
            <inertial pos="0 0 0" mass="0.1" diaginertia="0.002 0.002 0.002"/>           
            <!-- bottom -->
            <geom name="box_bottom" type="box" size="0.02 0.02 0.002" pos="0 0 0" rgba="0.7 0.7 0.7 0.5"/>
            <!-- side 01 (Front) -->
            <geom name="box_side01" type="box" size=".02 .001 .04"  pos="0 -0.0195 0.042"  rgba="0 0 .8 .5"/>    
            <!-- side 02 (Back) -->
            <geom name="box_side02" type="box" size=".02 .001 .04"  pos="0 0.0195 0.042"  rgba="0 0 .8 1"/>    
            <!-- side 03 (Left) -->
            <geom name="box_side03" type="box" size=".001 .02 .04"  pos="-0.0195 0 0.042"  rgba="0 0 .8 1"/>    
            <!-- side 04 (Right) -->
            <geom name="box_side04" type="box" size=".001 .02 .04"  pos="0.0195 0 0.042"  rgba="0 0 .8 1"/>    
        </body>

        <body name="TUBE_COVER" pos="0.0 .6 0.01">
            <joint name="tube_cover_joint" type="free" frictionloss="0.01"/>
            <inertial pos="0 0 0" mass="0.01" diaginertia="0.001 0.001 0.001"/>    
            <!-- top -->
            <geom name="box_cover" condim="4" solimp="0.99 1 0.01" solref="0.01 1" friction="0.5 0.5 0.01" type="box" size="0.02 0.02 0.002" pos="0 0 0" rgba="0.7 0.7 0.7 0.5"/>
            <!-- handle (short pillar) -->
            <geom name="box_handle" condim="4" solimp="0.99 1 0.01" solref="0.01 1" friction="0.5 0.5 0.01" type="box" size="0.005 0.005 0.02" pos="0 0 0.021" rgba="0.8 0.2 0.2 1"/>
        </body>

        <body name="TUBE" pos="-0.1 .6 0.01">
            <joint name="tube_joint" type="free" frictionloss="0.01"/>
            <inertial pos="0 0 0" mass="0.01" diaginertia="0.001 0.001 0.001"/>    
            <geom name="tube" condim="4" solimp="0.99 1 0.01" solref="0.01 1" friction="0.5 0.5 0.01" type="box" size="0.01 0.01 0.02" pos="0 0 0.021" rgba="0.2 0.8 0.2 1"/>
        </body>

    </worldbody>

    <actuator>
